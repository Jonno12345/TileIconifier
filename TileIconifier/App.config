<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <!--Enable the high DPI improvements introduced in .net 4.5.2. This can be 
    removed when targeting .net 4.7 or higher with the DpiAwarness key set to 
    PerMonitorV2.-->
    <add key="EnableWindowsFormsHighDpiAutoResizing" value="true" />
  </appSettings>
</configuration>